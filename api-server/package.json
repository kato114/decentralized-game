{
  "name": "decentral-games-server",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7.21.1"
  },
  "private": true,
  "scripts": {
    "start": "npm install && npm run build && node dist/index.js",
    "start:dev": "npm install && ts-node-dev index.ts",
    "start:production": "node dist/index.js",
    "start:docker": "sudo docker-compose down && sudo docker-compose build && sudo docker-compose up",
    "start:docker:rebuild": "sudo docker-compose down && sudo docker-compose build --no-cache && sudo docker-compose up --force-recreate",
    "build": "tsc -p tsconfig.json",
    "npm:update:all": "npm-update-all && npm i",
    "xdgDrop": "ts-node-dev modules/xdg/dropHelper.ts",
    "whitelist": "ts-node-dev modules/ice/whitelistScript.ts",
    "test": "jest",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.2.5",
    "@types/http-server": "^0.12.1",
    "@types/ioredis": "^4.28.8",
    "abort-controller": "3.0.0",
    "analytics-node": "5.1.0",
    "async": "3.2.3",
    "axios": "0.25.0",
    "bignumber.js": "9.0.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "crypto-js": "4.1.1",
    "decentraland-crypto-middleware": "^1.0.2",
    "eth-sig-util": "3.0.1",
    "ethereumjs-common": "1.5.2",
    "ethereumjs-tx": "2.1.2",
    "ethereumjs-util": "7.1.0",
    "ethers": "^5.5.4",
    "express": "4.17.1",
    "express-useragent": "1.0.15",
    "express-validator": "^6.14.0",
    "follow-redirects": "1.14.7",
    "fs": "0.0.2",
    "fs-extra": "10.0.0",
    "http": "0.0.1-security",
    "ioredis": "4.27.9",
    "json2csv": "5.0.6",
    "jsonwebtoken": "8.5.1",
    "keccak256": "1.0.3",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mongo-sanitize": "1.1.0",
    "mongoose": "6.0.7",
    "mongoose-int32": "0.5.0",
    "node-abort-controller": "3.0.1",
    "node-schedule": "2.0.0",
    "nodemon": "2.0.13",
    "puppeteer": "^13.5.1",
    "rate-limiter-flexible": "2.2.4",
    "request-ip": "2.1.3",
    "sleep-promise": "9.1.0",
    "swagger-ui-express": "4.2.0",
    "typescript": "4.5.5",
    "web3": "1.6.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/express": "4.17.13",
    "@types/node": "17.0.16",
    "@types/puppeteer": "^5.4.5",
    "@types/request": "2.48.7",
    "babel-jest": "27.5.1",
    "cross-env": "7.0.3",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "npm-update-all": "1.0.1",
    "supertest": "6.2.2",
    "ts-node": "10.2.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.5"
