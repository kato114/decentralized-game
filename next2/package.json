    "rimraf": "^3.0.2",
    "symlink-dir": "^5.0.1",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "packageManager": "npm@8.1.2"
}
