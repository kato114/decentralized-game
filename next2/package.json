{
  "name": "decentral-games-next-client",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15"
  },
  "private": true,
  "scripts": {
    "start": "npm run start:testing",
    "dev": "npm run start:testing",
    "start:localhost": "npm install && cross-env APP_ENV=localhost next dev",
    "start:testing": "npm install && cross-env APP_ENV=testing next dev",
    "start:development": "npm install && cross-env APP_ENV=development next dev",
    "start:staging": "npm install && cross-env APP_ENV=staging next dev",
    "start:production:debug": "npm install && cross-env APP_ENV=production next dev",
    "start:production": "next start -p 3000",
    "start:docker-compose": "sudo docker-compose down && sudo docker-compose build && sudo docker-compose up",
    "build:testing": "cross-env APP_ENV=testing next build",
    "build:development": "cross-env APP_ENV=development next build",
    "build:staging": "cross-env APP_ENV=staging next build",
    "build:production": "cross-env APP_ENV=production next build",
    "build:ci": "npm install && npm run build:production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint": "cross-env NODE_ENV=production npm run lint:all",
    "lint:all": "npm run lint:html && npm run lint:ts",
    "lint:ts": "cross-env TIMING=1 eslint './**/*.{ts,tsx,jsx}' --ignore-pattern '*.d.ts' --cache --fix",
    "lint:scss": "stylelint ./**/*.{css,scss} --fix",
    "lint:html": "prettier --write ./**/*.{tsx,html,js,jsx,json,md}",
    "prepare": "husky install",
    "postinstall": "husky install && rimraf .git/hooks && symlink-dir ../.husky .git/hooks",
    "npm:update:all": "npm-update-all && npm i",
    "tunnel": "local-ssl-proxy --source 3001 --target 3000"
  },
  "dependencies": {
    "@babel/core": "7.14.0",
    "@biconomy/mexa": "2.0.19",
    "@ramp-network/ramp-instant-sdk": "2.5.0",
