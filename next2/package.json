    "lodash": "4.17.21",
    "moment": "2.29.1",
    "next": "12.1.0",
    "next-seo": "5.1.0",
    "os-browserify": "0.3.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-burger-menu": "3.0.6",
    "react-chartjs-2": "2.11.1",
    "react-charts": "2.0.0-beta.7",
    "react-countdown": "2.3.1",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
