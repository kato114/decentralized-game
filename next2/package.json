    "react-i18next": "11.12.0",
    "react-input-autosize": "3.0.0",
    "react-lottie": "1.2.3",
    "react-mailchimp-subscribe": "2.1.3",
    "react-multi-carousel": "2.6.5",
    "react-parallax": "3.3.0",
    "react-scroll-parallax": "2.4.0",
    "react-slick": "0.28.1",
