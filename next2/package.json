    "assert": "2.0.0",
    "axios": "0.21.4",
    "bignumber.js": "9.0.1",
    "bootstrap": "5.0.0",
    "bootstrap-icons": "1.5.0",
    "buttercms": "1.2.7",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "crypto-browserify": "3.12.0",
    "env-cmd": "10.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "eth-sig-util": "3.0.1",
    "ethers": "5.4.6",
    "html-to-react": "1.4.5",
    "https-browserify": "1.0.0",
    "i18next": "20.6.0",
